#include<stdio.h>
int main()
{
    int n,k;
    int b;
    int avg1,sum1=0;
    scanf("%d %d",&n,&k);
    int a[n];
    for(int i=0;i<n;i++)
    {
scanf("%d ",&a[i]);
sum1+=a[i];
    }
sum1=sum1-a[k];
avg1=sum1/2;
scanf("%d",&b);
if(b>avg1)
printf("%d",b-avg1);
else 
printf("Bon Appetit");
return 0;
}

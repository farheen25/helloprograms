#include<stdio.h>
#include<stdlib.h>
int main()
{
    int x1,v1,x2,v2;
    int flag=0;
    scanf("%d %d %d %d",&x1,&v1,&x2,&v2);
    for(int i=0;i<10000;i++)
    {
        x1=x1+v1;
        x2=x2+v2;
    
        if(x1==x2)
            flag=1;   
    }
    if(flag==1)
    printf("YES");
    else
    printf("NO");
    return 0;
}
